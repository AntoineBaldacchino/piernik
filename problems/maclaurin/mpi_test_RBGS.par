 $DOMAIN_SIZES
    nxd = 64
    nyd = 32
    nzd = 16
 /
 $MPI_BLOCKS
    psize = 2, 2, 2
 /

 $CONSTANTS
    constants_set = "cgs"
 /

 $RESTART_CONTROL
    restart  = 'none'
    new_id   = ''
    nrestart = 0
 /

 $END_CONTROL
    tend   = 1.0
    nend   = 1
 /

 $OUTPUT_CONTROL
    problem_name ='maclaurin'
    run_id =  'sph'
    ix      = 0
    iy      = 0
    iz      = 0
    dt_hdf  = 10.0
    dt_res  = 0.0
    dt_plt  = 0.0
    dt_log  = 0.001
    dt_tsl  = 0.001
    vars(1) = 'dens'
    vars(2) = 'gpot'
    vars(3) = 'apot'
    vars(4) = 'errp'
    domain  = 'phys_domain'
 /

 $BOUNDARIES
    bnd_xl = 'out'
    bnd_xr = 'out'
    bnd_yl = 'out'
    bnd_yr = 'out'
    bnd_zl = 'out'
    bnd_zr = 'out'
 /

 $DOMAIN_LIMITS
    xmin   = -2.
    xmax   =  2.
    ymin   = -2.
    ymax   =  2.
    zmin   = -2.
    zmax   =  2.
 /

 $FLUID_IONIZED
    gamma_ion  = 1.67
    selfgrav_ion = .true.
 /

 $FLUID_NEUTRAL
    gamma_neu = 1.67
    selfgrav_neu = .false.
 /

 $NUMERICAL_SETUP
    cfl    = 0.7
    smalld = 1.e-20
    smallei= 1.e-20
    limiter= 'vanleer'
 /

 $GRAVITY
 /

 $THERMAL
 /

 $RESISTIVITY
 /

 $PROBLEM_CONTROL
    x0     = 0.
    y0     = 0.
    z0     = 0.
    d0     = 1.
    a1     = 1.
    e      = 0.
    nsub   = 3
 /

 $MULTIGRID_SOLVER
    level_max = 3
    stdout    = .true.
    dirty_debug = .true.
 /

 $MULTIGRID_GRAVITY
    norm_tol  = 1.e-6
    nsmool    = 8
    overrelax     =  1.0054
    grav_bnd_str  = "isolated"
    prefer_rbgs_relaxation = .true.
    gb_solve_gather = .true.
    max_cycles = 100
    vcycle_abort  = 100.
    overrelax_x = 1.0023
    overrelax_z = 1.000
    overrelax_y = 1.06
 /
