    gamma(ifluid) = 1.0
#else /* ISO */
    iena(ifluid)=ienn(iter)
    aneut((iter-1)*nunpf+5)=ienn(iter)
    fadiab(iadiab)=ifluid
    iadiab=iadiab+1
    fluidtype = 'adiabatic'
#endif /* ISO */
    write(*,'(a6,i2,a11,a10)') 'fluid ',ifluid,' neutral   ',fluidtype
